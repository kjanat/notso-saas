{
  "name": "saas-chatbot-platform",
  "version": "0.0.1",
  "private": true,
  "description": "Multi-tenant AI-powered customer service chatbot platform",
  "keywords": [
    "saas",
    "chatbot",
    "ai",
    "multi-tenant",
    "customer-service"
  ],
  "homepage": "https://github.com/notso/saas-chatbot-platform",
  "bugs": {
    "url": "https://github.com/notso/saas-chatbot-platform/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/notso/saas-chatbot-platform.git"
  },
  "license": "PROPRIETARY",
  "author": "NotSo Team",
  "type": "module",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:api": "turbo run dev --filter=@saas/api",
    "dev:web": "turbo run dev --filter=@saas/customer-portal",
    "dev:admin": "turbo run dev --filter=@saas/platform-admin",
    "build": "turbo run build",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "clean": "turbo run clean && rm -rf node_modules",
    "db:generate": "turbo run db:generate",
    "db:push": "turbo run db:push",
    "db:migrate": "turbo run db:migrate",
    "db:seed": "turbo run db:seed",
    "docker:up": "./scripts/dev-setup.sh",
    "docker:down": "docker-compose down",
    "docker:reset": "docker-compose down -v && ./scripts/dev-setup.sh",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/node": "^24.0.13",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  }
}
