  "name": "@saas/api",
  "private": true,
  "scripts": {
    "build": "tsup",
    "db:generate": "echo 'Prisma client is generated in @saas/database package'",
    "db:migrate": "echo 'Run db:migrate from root'",
    "db:push": "echo 'Run db:push from root'",
    "db:seed": "tsx src/scripts/seed.ts",
    "dev": "tsx watch src/main.ts",
    "lint": "eslint src",
    "start": "node dist/main.js",
    "test": "vitest run",
    "test:unit": "vitest run src/**/*.test.ts --passWithNoTests",
    "test:integration": "vitest run tests/integration/**/*.test.ts",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit"
