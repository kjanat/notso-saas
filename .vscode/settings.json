{
  "eslint.workingDirectories": [{ "mode": "auto" }],
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.next": true,
    "**/build": true,
    "**/coverage": true,
    "**/dist": true,
    "**/node_modules": true
  },
  "jest.jestCommandLine": "pnpm test",
  "jest.rootPath": "./",
  "search.exclude": {
    "**/.next": true,
    "**/build": true,
    "**/coverage": true,
    "**/dist": true,
    "**/node_modules": true,
    "pnpm-lock.yaml": true
  },
  "todo-tree.general.tags": ["BUG", "HACK", "FIXME", "TODO", "XXX", "[ ]", "[x]"],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^[ \\t]*(-|\\d+.))\\s*($TAGS)",
  "typescript.tsdk": "node_modules/typescript/lib"
}
